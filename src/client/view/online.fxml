<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="470.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.OnlineController">
   <children>
      <VBox alignment="CENTER" layoutX="250.0" layoutY="100.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Online users">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="infoLabel" text="select one or more users to start a new match">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <ListView fx:id="usersList" maxWidth="150.0" prefHeight="220.0" prefWidth="50.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </ListView>
            <HBox alignment="CENTER" spacing="25.0">
               <children>
                  <Button fx:id="matchButton" mnemonicParsing="false" onAction="#handleMatchButton" prefWidth="120.0" text="Start match" />
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackButton" prefWidth="120.0" text="Back" />
               </children>
               <opaqueInsets>
                  <Insets top="25.0" />
               </opaqueInsets>
               <padding>
                  <Insets top="25.0" />
               </padding>
            </HBox>
            <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#handleRefreshButton" prefWidth="120.0" text="Refresh">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
